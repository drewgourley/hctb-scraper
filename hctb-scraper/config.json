{
  "name": "HCTB Scraper",
  "version": "2.1.0",
  "slug": "hctb_scraper",
  "description": "This add-on will add a background service which will periodically check for bus rides and scrape the data for use in HomeAssistant device trackers. Built by Drew Gourley",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/drewgourley/hctb-scraper",
  "homeassistant_api": true,
  "options": {
    "hctb_username": null,
    "hctb_password": null,
    "hctb_schoolcode": null,
    "default_lat": null,
    "default_lon": null
  },
  "schema": {
    "hctb_username": "str",
    "hctb_password": "str",
    "hctb_schoolcode": "str",
    "default_lat": "float",
    "default_lon": "float"
  }
}
